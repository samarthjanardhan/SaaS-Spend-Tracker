{
  "manifest_version": 2,

  "name": "SaaS Spend Tracker",
  "description": "This extension tracks overall spendings with charts to display spendings",
  "version": "2.0",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },

  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },

  "options_page": "options.html",

  "permissions": ["storage", "notifications"]
}
